{
  "module": {
    "name": "DownLoadList",
    "type": "feature",
    "description": "$string:module_desc",
    "mainElement": "DownLoadListAbility",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "DownLoadListAbility",
        "srcEntry": "./ets/downloadlistability/DownLoadListAbility.ets",
        "description": "$string:DownLoadListAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:DownLoadListAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET",
        "reason": "$string:reason_network",
        "usedScene": {
          "when": "inuse"
        }
      }
    ]
  }
}